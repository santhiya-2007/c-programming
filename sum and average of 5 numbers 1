#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
int a,b,c,d,e,sum;
    scanf("%d\t%d\t%d\t%d\t%d",&a,&b,&c,&d,&e);
    float avg;
    sum=a+b+c+d+e;
    avg=sum/5.0;
    printf("Sum of the 5 number is : %d\n",sum);
    printf("Average of the 5 number is : %.1f\n",avg);
    return 0;
}
