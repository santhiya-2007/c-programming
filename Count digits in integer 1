#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    long long num;
    int count = 0;
    scanf("%lld", &num);
    if (num < 1 || num > 9999999) {
        printf("Enter a Valid Input\n");
    } else {
        long long temp = num;
        while (temp != 0) {
            temp /= 10;
            count++;
        }
        printf("The count of the given integer is: %d\n", count);
    }

    return 0;
}
