#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int n, i, num;
    float sum = 0, mean;
    scanf("%d", &n);
    for(i = 0; i < n; i++) {
        scanf("%d", &num);
        sum += num;
    }
    mean = sum / n;
    printf("Array Mean Value is %.2f\n", mean);
    return 0;
    }
