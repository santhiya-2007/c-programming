#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int size;
    scanf("%d", &size);  
int arr[size];
    int evenCount = 0, oddCount = 0;
    for (int i = 0; i < size; i++) {
        scanf("%d", &arr[i]);
        if (arr[i] % 2 == 0)
            evenCount++;
        else
            oddCount++;
    }
    if (evenCount == size)
        printf("Even\n");
    else if (oddCount == size)
        printf("Odd\n");
    else
        printf("Mixed\n");
 return 0;
}
