#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int n, i, num, sum = 0;
    scanf("%d", &n);
    for(i = 0; i < n; i++) {
        scanf("%d", &num);
        if(num > 0) {
            sum += num * num; 
        }
    }
    printf("%d\n", sum);

    return 0;
}
