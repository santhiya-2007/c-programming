#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main(){
    char name[50];
    int m1, m2, m3, m4, m5;
    int total;
    float average;
    char grade[10];
    scanf("%s", name);
    scanf("%d %d %d %d %d", &m1, &m2, &m3, &m4, &m5);
    total = m1 + m2 + m3 + m4 + m5;
    average = total / 5.0;
    if (average == 100)
        sprintf(grade, "S");
    else if (average >= 90)
        sprintf(grade, "A");
    else if (average >= 80)
        sprintf(grade, "B");
    else if (average >= 70)
        sprintf(grade, "C");
    else if (average >= 60)
        sprintf(grade, "D");
    else if (average >= 50)
        sprintf(grade, "E");
    else
        sprintf(grade, "Fail");
    printf("Name of the Student:%s\n", name);
    printf("Total Mark:%d\n", total);
    printf("Average Mark:%.1f\n", average);
    printf("Grade Mark:%s\n", grade);

    return 0;
}
